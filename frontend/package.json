{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "npx prettier --write 'src/**/*.{ts, tsx}' && eslint --fix --ext .ts src && eslint --fix --ext .tsx src",
    "typechain": "bash -c \"function myfunc () { typechain --target=ethers-v6 --out-dir src/contracts/\\$1 ../contract/artifacts_\\$1/RakugakiNFT.json ../contract/artifacts_\\$1/RakugakiLayers.json; }; myfunc paris26; myfunc paris18; myfunc cancun26; \""
  },
  "eslintConfig": {
    "extends": [
      "./eslint-config-react-app",
      "./eslint-config-react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "./react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "./babel-preset-react-app"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^7.2.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "babel-jest": "^30.0.5",
    "babel-loader": "^10.0.0",
    "babel-plugin-named-asset-import": "^0.3.8",
    "bfj": "^9.1.1",
    "browserslist": "^4.24.2",
    "camelcase": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "dotenv": "^17.2.1",
    "dotenv-expand": "^12.0.1",
    "eslint": "^9.16.0",
    "eslint-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.2.0",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-resolve": "^30.0.5",
    "jest-watch-typeahead": "^3.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-normalize": "^13.0.1",
    "postcss-preset-env": "^10.1.1",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^19.0.0",
    "react-refresh": "^0.17.0",
    "resolve": "^1.22.8",
    "resolve-url-loader": "^5.0.0",
    "sass-loader": "^16.0.4",
    "semver": "^7.6.3",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "^5.8.3",
    "web-vitals": "^5.1.0",
    "webextension-polyfill": "^0.12.0",
    "webpack": "^5.97.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-manifest-plugin": "^5.0.0",
    "workbox-webpack-plugin": "^7.3.0"
  },
  "devDependencies": {
    "@metamask/detect-provider": "^2.0.0",
    "@metamask/providers": "^22.1.0",
    "@mui/icons-material": "^7.2.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "base64-js": "^1.5.1",
    "buffer": "^6.0.3",
    "eslint-config-prettier": "^10.1.8",
    "ethers": "^6.13.4",
    "prettier": "^3.4.2",
    "react-transition-group": "^4.4.5",
    "typechain": "^8.3.2",
    "url": "^0.11.4"
  }
}
